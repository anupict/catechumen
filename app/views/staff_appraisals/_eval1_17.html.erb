	</tr>
	
	<tr valign=bottom>
		<td class="leftcolwide" rowspan=2><label for="eval1_effective">Total</label></td>
		<td colspan=2><%= f.text_field :e1g2_total, :class => "subtotals", :size => 4, :readonly => true %><label> x 25 = <%= f.text_field :e1g2_percent, :size => 7, :readonly => true %>%</label></td>
	</tr>
	<tr valign=bottom>
		<td>&nbsp; <%= (@staff_appraisal.g2_questions)*10 %></td>
		<td class="pagenote"><BR/><BR/><BR/><BR/></td>
	</tr>
	
	
	
	<tr>
		<th><%= t 'evaluation.form.title_part5' %></th>
		<th>PPP</th>
		<th></th>
	</tr>
		
	<tr valign=bottom>
		<td class="leftcolwide"><label for="eval1_manage"><%= t 'evaluation.form.management' %></label></td>
		<td><%= f.text_field :e1g3q1, :class => "e1g3", :size => 5, :placeholder => 0 %></td>
		<td class="pagenote"><%= t 'evaluation.form.management_desc' %></td>
	</tr>
	
	<tr valign=bottom>
		<td class="leftcolwide"><label for="eval1_discipline"><%= t 'evaluation.form.discipline' %></label></td>
		<td><%= f.text_field :e1g3q2, :class => "e1g3", :size => 5, :placeholder => 0 %></td>
