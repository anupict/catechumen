	<tr  valign=bottom>
		<td class="leftcolwide">Totals</td>
		<td><%= f.text_field :e1_total, :size => 5, :readonly => true %></td>
		<td class="pagenote">Percent [Part II 50% and Part III 50%]<BR/><BR/><BR/><BR/></td>
	</tr>

</table>








<script type="text/javascript">
	$j(document).ready(function(){
			//show submit date
			$j(".g1").keyup(function() {  
			  var e1g1q1 = $j("#staff_appraisal_e1g1q1").val();
			  var e1g1q2 = $j("#staff_appraisal_e1g1q2").val();
				var e1g1q3 = $j("#staff_appraisal_e1g1q3").val();
				var e1g1q4 = $j("#staff_appraisal_e1g1q4").val();
				var e1g1q5 = $j("#staff_appraisal_e1g1q5").val();
				
				var ansP = document.getElementById("staff_appraisal_e1g1_percent");
				ansP.value = (((e1g1q1*1) + (e1g1q2*1) + (e1g1q3*1) + (e1g1q4*1) + (e1g1q5*1))/50)*50;
				
				var ansG = document.getElementById("staff_appraisal_e1g1_total");
				ansG.value = (e1g1q1*1) + (e1g1q2*1) + (e1g1q3*1) + (e1g1q4*1) + (e1g1q5*1);
				

			});
			
			$j(".e1g2").keyup(function() {  
			  var e1g2q1 = $j("#staff_appraisal_e1g2q1").val();
			  var e1g2q2 = $j("#staff_appraisal_e1g2q2").val();
				var e1g2q3 = $j("#staff_appraisal_e1g2q3").val();
				
				var ansH = document.getElementById("staff_appraisal_e1g2_total");
				ansH.value = (e1g2q1*1) + (e1g2q2*1) + (e1g2q3*1);
				
				var ansK = document.getElementById("staff_appraisal_e1g2_percent");
				ansK.value = (((e1g2q1*1) + (e1g2q2*1) + (e1g2q3*1))/30)*25;
			});
			
			$j(".e1g3").keyup(function() {  
			  var e1g3q1 = $j("#staff_appraisal_e1g3q1").val();
			  var e1g3q2 = $j("#staff_appraisal_e1g3q2").val();
				var e1g3q3 = $j("#staff_appraisal_e1g3q3").val();
				var e1g3q4 = $j("#staff_appraisal_e1g3q4").val();
				
				var ansL = document.getElementById("staff_appraisal_e1g3_total");
				ansL.value = (e1g3q1*1) + (e1g3q2*1) + (e1g3q3*1) + (e1g3q4*1);
				
				var ansM = document.getElementById("staff_appraisal_e1g3_percent");
				ansM.value = (((e1g3q1*1) + (e1g3q2*1) + (e1g3q3*1) + (e1g3q4*1))/40)*20;			
			});
			
			$j(".e1g4").keyup(function() {  
			  var e1g4q1 = $j("#staff_appraisal_e1g4").val();
				
				var ansN = document.getElementById("staff_appraisal_e1g4_percent");
				ansN.value = ((e1g4q1*1)/10)*5;
			});
			
			$j(".g1,.e1g2,.e1g3,.e1g4").keyup(function() {  
			  var sub1 = $j("#staff_appraisal_e1g1_percent").val();
			  var sub2 = $j("#staff_appraisal_e1g2_percent").val();
				var sub3 = $j("#staff_appraisal_e1g3_percent").val();
				var sub4 = $j("#staff_appraisal_e1g4_percent").val();
				
				var ansQ = document.getElementById("staff_appraisal_e1_total");
				ansQ.value = (sub1*1) + (sub2*1) + (sub3*1) + (sub4*1);
			});

	});
</script>


