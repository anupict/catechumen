<h1>Editing studentattendance</h1>

<% form_for(@studentattendance) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :timetable_id %><br />
    <%= f.text_field :timetable_id %>
  </p>
  <p>
    <%= f.label :student_id %><br />
    <%= f.text_field :student_id %>
  </p>
  <p>
    <%= f.label :attend %><br />
    <%= f.check_box :attend %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @studentattendance %> |
<%= link_to 'Back', studentattendances_path %>