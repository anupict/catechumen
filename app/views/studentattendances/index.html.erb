<h1>Listing studentattendances</h1>

<table>
  <tr>
    <th>Timetable</th>
    <th>Student</th>
    <th>Attend</th>
  </tr>

<% @studentattendances.each do |studentattendance| %>
  <tr>
    <td><%=h studentattendance.timetable_id %></td>
    <td><%=h studentattendance.student_id %></td>
    <td><%=h studentattendance.attend %></td>
    <td><%= link_to 'Show', studentattendance %></td>
    <td><%= link_to 'Edit', edit_studentattendance_path(studentattendance) %></td>
    <td><%= link_to 'Destroy', studentattendance, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New studentattendance', new_studentattendance_path %>